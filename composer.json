{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.278",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.5",
        "laravellegends/pt-br-validator": "^9.1",
        "league/flysystem": "^3.15",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mailersend/laravel-driver": "^2.2",
        "mailersend/mailersend": "^0.8.0",
        "nunomaduro/collision": "^6.4",
        "phpseclib/phpseclib": "^3.0",
        "phpunit/phpunit": "^9.4",
        "psr/http-message": "^1.0",
        "spatie/laravel-ignition": "^1.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^5.0",
        "laravel/breeze": "^1.9",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Jetimob\\Juno\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\Juno\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\Juno\\JunoServiceProvider"
            ],
            "aliases": {
                "Juno": "Jetimob\\Juno\\Facades\\Juno"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
